name: "m4"
version: "1.4.21"
release: "1"
description: "GNU m4"
sources:
 - "https://ftp.gnu.org/gnu/m4/m4-${version}.tar.gz"
sha256sum:
 - "38ae59f7a30bf9c108193cc5c25fbb06014f21e230c7ede2eff614f7b7c37ed8"
build_depends:
 - "gmake"

build {
 env CFLAGS="${CFLAGS} -std=gnu17 -Wno-attributes -D_GL_ATTRIBUTE_NODISCARD="
 macro build
}

package {
 macro package
 exec "rm -rf \"$ROOT/usr/share/info/dir\""
}


