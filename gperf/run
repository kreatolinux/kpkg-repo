#!/bin/sh
NAME="gperf"
RELEASE="1"
VERSION="3.3"
BUILD_DEPENDS="gmake"
SOURCES="https://ftp.gnu.org/pub/gnu/gperf/gperf-$VERSION.tar.gz"
DEPENDS=""
SHA256SUM="fd87e0aba7e43ae054837afd6cd4db03a3f2693deb3619085e6ed9d8d9604ad8"
DESCRIPTION="gperf"
build() {
    kpkgConfigure --prefix=/usr
    make
}

package() {
    make DESTDIR=$ROOT install
}
