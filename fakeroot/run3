name: "fakeroot"
version: "1.37.1.2"
release: "1"
description: "Tool for simulating superuser privileges"
sources:
 - "https://deb.debian.org/debian/pool/main/f/fakeroot/fakeroot_${version}.orig.tar.gz"
sha256sum:
 - "959496928c8a676ec8377f665ff6a19a707bfad693325f9cc4a4126642f53224"
depends:
 - "libcap"
build_depends:
 - "gmake"

build {
 env CFLAGS="$CFLAGS -Wno-incompatible-pointer-types"
 macro build --disable-static
}

package {
 macro package
}

func install {
 macro package
}


