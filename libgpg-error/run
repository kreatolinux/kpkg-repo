#!/bin/sh
NAME="libgpg-error"
RELEASE="1"
VERSION="1.58"
BUILD_DEPENDS="gmake"
SOURCES="https://gnupg.org/ftp/gcrypt/libgpg-error/libgpg-error-$VERSION.tar.bz2"
DEPENDS=""
SHA256SUM="f943aea9a830a8bd938e5124b579efaece24a3225ff4c3d27611a80ce1260c27"
DESCRIPTION="A small library that originally defined common error values for all GnuPG components"

build() {
    kpkgConfigure --prefix=/usr
    make -j$(nproc)
}

package() {
    make install
}
